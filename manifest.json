{
    "author": "Estate Diamond Jewelry",
    "background": {
        "page": "background.html",
        "persistent": true
    },
    "browser_action": {
        "default_icon": {
            "20": "icons/engagement-ring-wish-list-16.png"
        },
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
    "description": "The ultimate Wish List for choosing your dream ring.",
    "icons": {
        "16": "icons/engagement-ring-wish-list-16.png",
        "48": "icons/engagement-ring-wish-list-48.png",
        "128": "icons/engagement-ring-wish-list-128.png"
    },
    "manifest_version": 2,
    "name": "Engagement Ring Wish List",
    "permissions": [
        "storage",
        "contextMenus",
        "activeTab"
    ],
    "version": "1.0.3",
    "-ms-preload": {
        "backgroundScript": "backgroundScriptsAPIBridge.js",
        "contentScript": "contentScriptsAPIBridge.js"
    }
}